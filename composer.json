{
    "name": "oracle-monitor/oracle-monitor-sga-tablespace",
    "description": "Oracle Database Monitoring Tool with multi-tenancy features.",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Your Name/Company",
            "email": "your@email.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "symfony/var-dumper": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "server/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
